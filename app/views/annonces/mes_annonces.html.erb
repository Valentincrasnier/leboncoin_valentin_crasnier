<div class="row">
    <% @annonces.each do |f| %>
    <div class="col-md-4 thumbnail col-flex">
        <%= image_tag f.image  %><br/><br/>
        <p>Titre:
            <%= f.titre %>
        </p>
        <%= link_to "Editer",  edit_annonce_path(f.id), class: 'btn btn-success', method: :get %>
        <%= link_to "Supprimer", annonce_path(f.id), class: 'btn btn-danger', method: :delete %>  </div>
    <% end %>
</div>
